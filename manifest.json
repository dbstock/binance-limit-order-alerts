{
    "name": "Binance Order Filled Alert",
    "version": "1.0",
    "description": "Plays an audio alert when an order is placed, cancelled, or filled",
    "manifest_version": 2,
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
        {
          "matches": [
            "https://www.binance.com/en/trade/pro/*"
          ],
          "js": ["content.js"]
        }
      ]
}